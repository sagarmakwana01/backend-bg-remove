<section class="content">
    <div class="container-fluid">
        <div col-md-6>
            <div class="card card-primary">
                <div class="card-header">
                    <h3 class="card-title">Add Baby Frame</h3>
           
                </div>
                <div class="mt-3">
                    <% if (message) { %>
                        <div class="alert <%= message.type === 'success' ? 'alert-success' : 'alert-danger' %> alert-dismissible">
                            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                            <%= message.message %>
                        </div>
                    <% } %>
                </div>
                <!-- /.card-header -->
                <!-- form start -->
    
                
                <form action="/babyframe" method="POST" enctype="multipart/form-data" id="form-upload">
                    <div class="card-body">
                        <div class="form-group">
                            <label for="month">Month</label>
                            <select id="month" name="month" class="form-control" autocomplete="off">
                                multiple

                                <option value="1">1</option>
                                <option value="2">2</option>
                                <option value="3">3</option>
                                <option value="4">4</option>
                                <option value="5">5</option>
                                <option value="6">6</option>
                                <option value="7">7</option>
                                <option value="8">8</option>
                                <option value="9">9</option>
                                <option value="10">10</option>
                                <option value="11">11</option>
                                <option value="12">12</option>
                            </select>
                        </div>
                        <div class="form-group">
                            <label for="babyType">baby</label>
                            <select id="babyType" name="baby" class="form-control" required autocomplete="off">

                                <option value="boy">Boy</option>
                                <option value="girl">Girl </option>

                            </select>
                        </div>
                       <%= console.log(message) %>
                        <div class="form-group">
                            <label for="multiplefileupload" class="form-label">Frame Image</label>
                            <input id="multiplefileupload" name="image" type="file" accept=".jpg,.gif,.png,.svg" multiple autocomplete="off">
                        </div>
                        <div class="form-check">
                            <input type="checkbox" name="paid" class="form-check-input" id="exampleCheck1" autocomplete="off">
                            <label class="form-check-label" for="exampleCheck1">Paid </label>
                        </div>
                        <!-- /.card-body -->

                        <div class="card-footer mt-3">
                            <button type="submit" class="btn btn-primary">Submit</button>
                        </div>
                </form>
            </div>
        </div>
    </div>
    <!-- /.container-fluid -->
</section>